{
  "manifest_version": 2,

  "name": "PinIt",
  "description": "This extension will let you save important info based on its category",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "./popup/popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "chrome_url_overrides" : {
     "newtab": "./new_tab/index.html"
  }
}